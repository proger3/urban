def print_params(a=1, b='строка', c=True):
    print(f"a: {a}, b: {b}, c: {c}")

# Вызов функции без аргументов
print_params()
# Вызов функции с одним аргументом b
print_params(b=25)
# Вызов функции с одним аргументом c
print_params(c=[1, 2, 3])
# Вызов функции с двумя аргументами
print_params(a=10, b='другая строка')

# Создание списка с тремя элементами разных типов
values_list = [42, 'текст', False]
# Создание словаря с ключами, соответствующими параметрам функции
values_dict = {'a': 99, 'b': 'словарь строка', 'c': [10, 20, 30]}
# Передача списка с использованием распаковки
print_params(*values_list)
# Передача словаря с использованием распаковки
print_params(**values_dict)

# Создание списка с двумя элементами
values_list_2 = [3.14, 'параметр из списка']
# Передача списка с двумя элементами и дополнительного параметра
print_params(*values_list_2, 42)
